    478          //*****************************************************************************
    479          void
    480          am_bsp_iom_pins_disable(uint32_t ui32Module, am_hal_iom_mode_e eIOMMode)
    481          {
    482              uint32_t ui32Combined;
    483          
    484              //
    485              // Validate parameters
    486              //
    487              if ( ui32Module >= AM_REG_IOM_NUM_MODULES )
    488              {
    489                  //
    490                  // FPGA supports only IOM0 and 1.
    491                  //
    492                  return;
